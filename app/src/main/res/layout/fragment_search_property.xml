<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_low"
    android:layoutDirection="locale">

    <include
        android:id="@+id/in_toolbar"
        layout="@layout/layout_toolbar_main"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/in_toolbar">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="@dimen/_86sdp">

            <TextView
                android:id="@+id/textView49"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_8sdp"
                android:text="@string/advanced_search"
                android:textColor="@color/amtalek_blue_dark"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/et1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_16sdp"
                android:background="@drawable/shape_white"
                android:hint="@string/main_word"
                android:inputType="text"
                android:padding="@dimen/_6sdp"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView49"
                android:importantForAutofill="no" />

            <include
                android:id="@+id/in_spinner_location"
                layout="@layout/layout_default_spinner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                app:layout_constraintEnd_toEndOf="@+id/et1"
                app:layout_constraintStart_toStartOf="@+id/et1"
                app:layout_constraintTop_toBottomOf="@+id/et1" />

            <include
                android:id="@+id/in_spinner_type"
                layout="@layout/layout_default_spinner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                app:layout_constraintEnd_toEndOf="@+id/et1"
                app:layout_constraintStart_toStartOf="@+id/et1"
                app:layout_constraintTop_toBottomOf="@+id/in_spinner_location" />

            <include
                android:id="@+id/in_spinner_status"
                layout="@layout/layout_default_spinner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                app:layout_constraintEnd_toEndOf="@+id/et1"
                app:layout_constraintStart_toStartOf="@+id/et1"
                app:layout_constraintTop_toBottomOf="@+id/in_spinner_type" />

            <include
                android:id="@+id/in_spinner_currency"
                layout="@layout/layout_default_spinner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                app:layout_constraintEnd_toEndOf="@+id/et1"
                app:layout_constraintStart_toStartOf="@+id/et1"
                app:layout_constraintTop_toBottomOf="@+id/in_spinner_status" />

            <include
                android:id="@+id/in_spinner_bathroom"
                layout="@layout/layout_default_spinner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_8sdp"
                app:layout_constraintEnd_toStartOf="@+id/in_spinner_beds"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/in_spinner_currency" />

            <include
                android:id="@+id/in_spinner_beds"
                layout="@layout/layout_default_spinner"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="@+id/in_spinner_currency"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/in_spinner_bathroom"
                app:layout_constraintTop_toTopOf="@+id/in_spinner_bathroom" />

            <EditText
                android:id="@+id/et2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:background="@drawable/shape_white"
                android:hint="@string/max_area"
                android:inputType="text"
                android:padding="@dimen/_6sdp"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintEnd_toEndOf="@+id/in_spinner_bathroom"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/in_spinner_bathroom"
                app:layout_constraintTop_toBottomOf="@+id/in_spinner_bathroom"
                android:importantForAutofill="no" />

            <EditText
                android:id="@+id/et3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:background="@drawable/shape_white"
                android:hint="@string/min_area"
                android:inputType="text"
                android:padding="@dimen/_6sdp"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintEnd_toEndOf="@+id/in_spinner_beds"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/in_spinner_beds"
                app:layout_constraintTop_toBottomOf="@+id/in_spinner_beds"
                android:importantForAutofill="no" />

            <EditText
                android:id="@+id/et4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:background="@drawable/shape_white"
                android:hint="@string/max_price"
                android:inputType="text"
                android:padding="@dimen/_6sdp"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintEnd_toEndOf="@+id/in_spinner_bathroom"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/in_spinner_bathroom"
                app:layout_constraintTop_toBottomOf="@+id/et2"
                android:importantForAutofill="no" />

            <EditText
                android:id="@+id/et5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8sdp"
                android:background="@drawable/shape_white"
                android:hint="@string/min_price"
                android:inputType="text"
                android:padding="@dimen/_6sdp"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintEnd_toEndOf="@+id/in_spinner_beds"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/in_spinner_beds"
                app:layout_constraintTop_toBottomOf="@+id/et3"
                android:importantForAutofill="no" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="@dimen/_22sdp"
                android:layout_height="@dimen/_22sdp"
                android:layout_marginTop="@dimen/_16sdp"
                android:background="@drawable/shape_yellow"
                android:padding="@dimen/_4sdp"
                app:layout_constraintStart_toStartOf="@+id/et4"
                app:layout_constraintTop_toBottomOf="@+id/et5"
                app:srcCompat="@drawable/ic_plus"
                android:contentDescription="@null"/>

            <TextView
                android:id="@+id/textView50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8sdp"
                android:text="@string/amenities_options"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView20"
                app:layout_constraintStart_toEndOf="@+id/imageView20"
                app:layout_constraintTop_toTopOf="@+id/imageView20" />

            <TextView
                android:id="@+id/tv_search"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_32sdp"
                android:layout_marginTop="@dimen/_16sdp"
                android:background="@drawable/shape_yellow"
                android:gravity="center"
                android:paddingVertical="@dimen/_8sdp"
                android:text="@string/search"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView20" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>