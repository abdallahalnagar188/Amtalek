        android:id="@+id/nyEstateRentFragment"
        android:name="eramo.amtalek.presentation.ui.main.myestate.MyEstateRentFragment"
        android:label="NewsDetailsFragment"
        tools:layout="@layout/fragment_my_estate_rent" />
